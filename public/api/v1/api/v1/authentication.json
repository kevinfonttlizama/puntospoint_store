{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://localhost:3000",
  "resourcePath": "authentication",
  "apis": [
    {
      "path": "/api/v1/authenticate.json",
      "operations": [
        {
          "summary": "Autenticar administrador y obtener un token JWT",
          "notes": "Devuelve un token JWT para usar en solicitudes posteriores",
          "parameters": [
            {
              "paramType": "form",
              "name": "email",
              "type": "string",
              "description": "Correo electronico del administrador",
              "required": true
            },
            {
              "paramType": "form",
              "name": "password",
              "type": "string",
              "description": "Contrasena del administrador",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Autenticacion exitosa"
            },
            {
              "code": 401,
              "responseModel": null,
              "message": "Credenciales invalidas"
            }
          ],
          "nickname": "Api::V1::Authentication#authenticate",
          "method": "post"
        }
      ]
    }
  ],
  "authorizations": null
}